!Database methodsFor!
find: term index: index
	" Try to find unfication wirh some of rules from database, starting matching from index"
	| len  work i t h p|
	p := Pair new.
	i := index.
	len := rules size.
	work := true.
	[(i <= len) & (work)] whileTrue:
			[
			  t := (rules at: i) trans.
			  h := t car head. 
			  term go: h do: [work := false. p car: (t car body) cdr: i . ^p].
			  "V delete: t cdr."
			  i := i + 1.
			].
	^Error
	! !
